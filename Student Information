	String ClassTeacherName = "Mrs.Khilari";
	int StudentPhysicsMark = 70;
	int StudentMathMark = 50;
	int StudentChemistyMark = 55;
	int StudentBiologyMark = 30;
	void StudInfo() 
	{
	 System.out.println("");
	 System.out.println("Student Information for TYBCS");
	 System.out.println("Student Roll no is "+StudRollno);
	 System.out.println("Student Name is "+StudName);
	 System.out.println("Student ClassName is "+ClassName);
	 System.out.println("Student ClassTeacherName is "+ClassTeacherName);
	 System.out.println("Student StudentPhysicsMark is "+StudentPhysicsMark);
	 System.out.println("Student StudentMathMark is "+StudentMathMark);
	 System.out.println("Student StudentChemistyMark is "+StudentChemistyMark);
	 System.out.println("Student StudentBiologyMark is "+StudentBiologyMark);
	} 
}
public class Student
{
	public static void main(String[] args) 
	{
		FYBCS f1 = new FYBCS();
		SYBCS s1 = new SYBCS();
		TYBCS t1 = new TYBCS();
		f1.StudInfo();
		s1.StudInfo();
		t1.StudInfo();
		}
  
}
